{
  "name": "datalogger_jupyterlab",
  "private": true,
  "version": "0.1.0",
  "description": "JupyterLab extension for DataLogger",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/PainterQubits/datalogger-jupyterlab-exploration",
  "bugs": {
    "url": "https://github.com/PainterQubits/datalogger-jupyterlab-exploration/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PainterQubits/datalogger-jupyterlab-exploration.git"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alex Hadley",
    "email": "alexhad6@gmail.com"
  },
  "type": "module",
  "jupyterlab": {
    "extension": true,
    "outputDir": "labextension"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -b --sourceMap && yarn build:labextension",
    "build:prod": "tsc -b --clean && rm -rf lib && tsc -b && yarn build:labextension",
    "build:labextension": "hatch run jupyter labextension build .",
    "lint": "prettier --check ."
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.5",
    "react": "^18.2.0"
  },
  "packageManager": "yarn@3.6.3",
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
