{
  "name": "datalogger-jupyterlab",
  "private": true,
  "version": "0.1.0",
  "description": "JupyterLab extension for DataLogger.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/PainterQubits/datalogger-jupyterlab",
  "bugs": {
    "url": "https://github.com/PainterQubits/datalogger-jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PainterQubits/datalogger-jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alex Hadley",
    "email": "alexhad6@gmail.com"
  },
  "type": "module",
  "main": "lib/index.js",
  "jupyterlab": {
    "extension": true,
    "outputDir": "labextension",
    "schemaDir": "schema"
  },
  "scripts": {
    "dev": "yarn symlink:labextension && run-p watch:src watch:labextension jupyterlab",
    "build": "rm -rf lib tsconfig.tsbuildinfo && tsc && hatch run jupyter labextension build .",
    "preview": "yarn build && hatch env prune && yarn jupyterlab",
    "lint": "prettier --check .",
    "symlink:labextension": "hatch run jupyter labextension develop --overwrite .",
    "watch:src": "tsc -w --sourceMap",
    "watch:labextension": "hatch run jupyter labextension watch .",
    "jupyterlab": "hatch run jupyter lab"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.5",
    "@jupyterlab/apputils": "^4.1.5",
    "@jupyterlab/filebrowser": "^4.0.5",
    "@jupyterlab/ui-components": "^4.0.5"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@3.6.3"
}
