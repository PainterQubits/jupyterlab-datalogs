{
  "name": "datalogger_jupyterlab",
  "private": true,
  "version": "0.1.0",
  "description": "JupyterLab extension for DataLogger",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/PainterQubits/datalogger-jupyterlab-exploration",
  "bugs": {
    "url": "https://github.com/PainterQubits/datalogger-jupyterlab-exploration/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PainterQubits/datalogger-jupyterlab-exploration.git"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alex Hadley",
    "email": "alexhad6@gmail.com"
  },
  "type": "module",
  "jupyterlab": {
    "extension": true,
    "outputDir": "labextension"
  },
  "main": "lib/index.js",
  "scripts": {
    "dev": "yarn symlink:labextension && run-p watch:src watch:labextension jupyter:lab",
    "build": "rm -rf lib tsconfig.tsbuildinfo && tsc && hatch run jupyter labextension build .",
    "lint": "prettier --check .",
    "symlink:labextension": "hatch run jupyter labextension develop --overwrite .",
    "watch:src": "tsc -w --sourceMap",
    "watch:labextension": "hatch run jupyter labextension watch .",
    "jupyter:lab": "hatch run jupyter lab"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.5",
    "react": "^18.2.0"
  },
  "packageManager": "yarn@3.6.3",
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
